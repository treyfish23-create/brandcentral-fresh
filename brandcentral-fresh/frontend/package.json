{
  "name": "rollodx-frontend-enterprise",
  "version": "1.0.0",
  "description": "ROLLodx Enterprise B2B SaaS Frontend - Complete React application for brand management platform",
  "private": true,
  "homepage": "https://rollodx.vercel.app",
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "preview": "npm run build && serve -s build",
    "deploy": "npm run build && vercel --prod"
  },
  "keywords": [
    "react",
    "b2b",
    "saas",
    "brand-management",
    "cannabis",
    "retail",
    "crm",
    "frontend",
    "enterprise",
    "dashboard"
  ],
  "author": {
    "name": "Trey Fisher",
    "email": "trey@rollodx.com",
    "url": "https://rollodx.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rollodx/rollodx-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/rollodx/rollodx-frontend/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "axios": "^1.6.2",
    "react-hot-toast": "^2.4.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "@tanstack/react-query": "^5.8.4",
    "react-intersection-observer": "^9.5.3",
    "react-dropzone": "^14.2.3",
    "react-beautiful-dnd": "^13.1.1",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.8",
    "react-select": "^5.8.0",
    "react-datepicker": "^4.21.0",
    "react-image-gallery": "^1.3.0",
    "react-cropper": "^2.3.3",
    "recharts": "^2.8.0",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "d3": "^7.8.5",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "classnames": "^2.3.2",
    "prop-types": "^15.8.1",
    "web-vitals": "^3.5.0",
    "workbox-webpack-plugin": "^7.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/d3": "^7.4.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "typescript": "^5.3.3",
    "serve": "^14.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "@craco/craco": "^7.1.0",
    "craco-alias": "^3.0.1",
    "bundle-analyzer": "^4.0.0",
    "source-map-explorer": "^2.5.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "prefer-const": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "proxy": "http://localhost:3001",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/**/*.test.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
