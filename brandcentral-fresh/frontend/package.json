{
  "name": "rollodx-frontend-enterprise",
  "version": "1.0.0",
  "description": "ROLLodx Enterprise B2B SaaS Frontend - Complete React application for brand management platform with 100+ components",
  "private": true,
  "homepage": "https://rollodx.vercel.app",
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "build": "react-scripts build && npm run generate-sitemap",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "analyze:size": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "preview": "npm run build && serve -s build -p 3000",
    "deploy": "npm run build && vercel --prod",
    "deploy:staging": "npm run build && vercel",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "pre-commit": "lint-staged",
    "postinstall": "patch-package",
    "clean": "rimraf build node_modules/.cache",
    "reset": "npm run clean && npm install"
  },
  "keywords": [
    "react",
    "typescript",
    "b2b",
    "saas",
